<div class="users-index-wrapper">
  <div class="container">
    <h1 class="users-heading">ユーザー一覧</h1>
    <% @users.each do |user| %>
      <div class="row">
        <div class="col-md-6 col-md-offset-3">
          <div class="user-index-single">
            <p class="user-right">
              <%#= link_to user_path(user_id: user.id) do %>
              <%= link_to "#" do %>
                <%= user.name %>
              <% end %>
            </p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>