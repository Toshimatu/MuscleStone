<div class="user-edit-wrapper">
  <div class="container">
    <div class="row">
      <div class="user-edit-heading">
        <div class="col-md-6 col-md-offset-3">
          <h2 class="title">プロフィール編集</h2>
          <%= form_for @user do |f| %>
            <div class="form-group">
              <%= f.label :名前, class: 'text-brack' %>
              <%= f.text_field :name, class: 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :email, class: 'text-brack' %>
              <%= f.text_field :email, class: 'form-control' %>
            </div>

          　<%= f.submit "保存", class: 'btn-block' %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>